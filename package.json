{
  "name": "modloader64",
  "version": "1.0.0",
  "description": "",
  "main": "./src/index.js",
  "scripts": {
    "tsc": "tsc --listEmittedFiles",
    "modules": "npm run build && node ./build.js --step=pushModules",
    "build": "tsc ./build.ts --esModuleInterop && node ./build.js --step=prebuild && node ./build.js --step=build && npm run tsc && node ./build.js --step=postbuild",
    "build_api": "tsc ./build.ts --esModuleInterop && node ./build.js --step=forceAPI",
    "start": "npm run build && node ./build/src/index.js --dir=build",
    "start_1": "node ./build/src/index.js --dir=build",
    "start_2": "node ./build2/src/index.js --dir=build2",
    "start_bundle": "npm run build && build\\libs\\node.exe ./build/src/index.js --dir=build",
    "test": "npm run build && tsc ./test/TestMupenRam.ts && mocha ./test/TestMupenRam.js && tsc ./test/TestMupenRom.ts && mocha ./test/TestMupenRom.js",
    "fix": "gts fix",
    "dist": "npm run fix && npm run modules",
	"postinstall": "npm install && npm install --save ./API/build"
  },
  "author": "Team-Ooto",
  "dependencies": {
    "bitwise": "^2.0.3",
    "buffer-json": "^2.0.0",
    "eventemitter2": "^5.0.1",
    "fs-extra": "^8.1.0",
    "human-readable-ids": "^1.0.4",
    "mkdir-recursive": "^0.4.0",
    "nat-upnp": "^1.1.1",
    "socket.io": "^2.2.0",
    "socket.io-client": "^2.2.0",
    "socket.io-encrypt": "^0.1.0",
    "source-map-support": "^0.5.13",
    "uuid": "^3.3.2",
    "simple-node-logger": "^18.12.23",
    "commander": "^3.0.2",
    "download-file": "^0.1.5",
    "request": "^2.88.0",
    "ncp": "^2.0.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^8.0.0",
    "@types/ncp": "^2.0.1",
    "@types/node": "^12.0.10",
    "@types/socket.io": "^2.1.2",
    "@types/uuid": "^3.4.5",
    "copy-node-modules": "^1.1.1",
    "find-remove": "^1.2.2",
    "gts": "^1.0.0",
    "recursive-readdir": "^2.2.2",
    "typescript": "^3.5.2",
    "@types/request": "^2.48.3"
  }
}
