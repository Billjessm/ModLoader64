{
  "name": "modloader64",
  "version": "1.0.0",
  "description": "",
  "main": "./src/index.js",
  "scripts": {
    "tsc": "tsc --listEmittedFiles",
    "modules": "npm run build && node ./build.js --step=pushModules",
    "build": "tsc ./build.ts --esModuleInterop && node ./build.js --step=prebuild && node ./build.js --step=build && npm run tsc && node ./build.js --step=postbuild",
    "build_api": "tsc ./build.ts --esModuleInterop && node ./build.js --step=forceAPI",
    "start": "npm run build && node ./build/src/index.js --mode=cli --dir=build",
    "start_2": "node ./build2/src/index.js --mode=cli --dir=build2",
    "start_bundle": "npm run build && build\\libs\\node.exe ./build/src/index.js --mode=cli --dir=build",
    "start_debug": "node ./build/src/index.js --mode=cli --dir=build",
    "test": "npm run build && tsc ./test/TestMupenRam.ts && mocha ./test/TestMupenRam.js && tsc ./test/TestMupenRom.ts && mocha ./test/TestMupenRom.js",
    "fix": "gts fix"
  },
  "pre-commit": [
    "build_api",
    "fix",
    "build"
  ],
  "author": "Team-Ooto",
  "dependencies": {
    "@types/deep-equal": "^1.0.1",
    "bitwise": "^2.0.3",
    "buffer-json": "^2.0.0",
    "commander": "^2.20.0",
    "deep-equal": "^1.0.1",
    "express": "^4.17.1",
    "human-readable-ids": "^1.0.4",
    "modloader64_api": "file:API/build",
    "nat-upnp": "^1.1.1",
    "ncp": "^2.0.0",
    "portfinder": "^1.0.21",
    "socket.io": "^2.2.0",
    "socket.io-client": "^2.2.0",
    "socket.io-encrypt": "^0.1.0",
    "socket.io-fix-close": "0.0.2",
    "uuid": "^3.3.2",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.0",
    "@types/mocha": "^5.2.7",
    "@types/ncp": "^2.0.1",
    "@types/node": "^12.0.10",
    "@types/socket.io": "^2.1.2",
    "@types/uuid": "^3.4.5",
    "copy-node-modules": "^1.1.1",
    "find-remove": "^1.2.2",
    "gc-profiler": "^1.4.1",
    "gts": "^1.0.0",
    "mocha": "^6.2.0",
    "pre-commit": "^1.2.2",
    "recursive-readdir": "^2.2.2",
    "source-map-support": "^0.5.13",
    "thread-sleep": "^2.2.0",
    "typescript": "^3.5.2",
    "v8-profiler-next": "^1.1.1"
  }
}
