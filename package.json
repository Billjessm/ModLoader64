{
  "name": "modloader64",
  "version": "1.0.0",
  "description": "",
  "main": "./src/index.js",
  "scripts": {
    "tsc": "tsc --listEmittedFiles",
    "modules": "npm run build && node ./build.js --step=pushModules",
    "build": "tsc ./build.ts --esModuleInterop && node ./build.js --step=prebuild && node ./build.js --step=build && npm run tsc && node ./build.js --step=postbuild",
    "build_api": "tsc ./API/build.ts --esModuleInterop && cd ./API && node ./build.js --step=prebuild && node ./build.js --step=build && npm run tsc && node ./build.js --step=postbuild && cd ./build && npm link",
    "start": "node ./build/src/index.js --dir=build",
    "start_1": "node ./build/src/index.js --dir=build",
    "start_2": "node ./build.js --step=player2 && node ./build2/src/index.js --dir=build2",
    "fix": "npx eslint src/**/*.ts --fix",
    "dist": "npm run build_api && npm run fix && npm run modules",
    "postinstall": "npx patch-package && npm run build_api && tsc ./build.ts --esModuleInterop && node ./build.js --step=installEmulator && npx tar -xzvf ./Mupen64Plus/emulator.tar.gz -C ./Mupen64Plus && npm link modloader64_api && tsc ./bin/index.ts --esModuleInterop"
  },
  "bin": {
    "modloader64": "./bin/index.js"
  },
  "author": "Team-Ooto",
  "dependencies": {
    "bitwise": "^2.0.3",
    "buffer-json": "^2.0.0",
    "eventemitter2": "^5.0.1",
    "fs-extra": "^8.1.0",
    "human-readable-ids": "^1.0.4",
    "mkdir-recursive": "^0.4.0",
    "nat-upnp": "^1.1.1",
    "socket.io": "^2.2.0",
    "socket.io-client": "^2.2.0",
    "socket.io-encrypt": "^0.1.0",
    "source-map-support": "^0.5.13",
    "uuid": "^3.3.2",
    "simple-node-logger": "^18.12.23",
    "commander": "^3.0.2",
    "download-file": "^0.1.5",
    "request": "^2.88.0",
    "ncp": "^2.0.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^8.0.0",
    "@types/ncp": "^2.0.1",
    "@types/node": "^12.12.25",
    "@types/request": "^2.48.3",
    "@types/socket.io": "^2.1.2",
    "@types/uuid": "^3.4.5",
    "@typescript-eslint/eslint-plugin": "^2.17.0",
    "@typescript-eslint/parser": "^2.17.0",
    "copy-node-modules": "^1.1.1",
    "eslint": "^6.8.0",
    "find-remove": "^1.2.2",
    "modloader64-platform-deps": "github:hylian-modding/ModLoader64-Platform-Deps",
    "patch-package": "^6.2.0",
    "recursive-readdir": "^2.2.2",
    "tar": "^5.0.5",
    "typescript": "^3.5.2"
  }
}
